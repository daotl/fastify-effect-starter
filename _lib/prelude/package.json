{
  "name": "@fastify-effect-starter/prelude",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "scripts": {
    "clean:all": "concurrently 'pnpm clean' 'pnpm clean:test'",
    "clean:src": "rimraf dist",
    "clean:test": "rimraf _test/dist",
    "build:all": "pnpm clean:all && NODE_OPTIONS=--max-old-space-size=6144 tsc --build",
    "build": "pnpm clean:src && NODE_OPTIONS=--max-old-space-size=6144 tsc",
    "build:test": "pnpm clean:test && NODE_OPTIONS=--max-old-space-size=6144 tsc -p tsconfig.test.json"
  },
  "dependencies": {
    "@effect-app/core": "^0.36.0",
    "@effect-app/infra": "^0.41.1",
    "@effect-app/infra-adapters": "^0.29.1",
    "@effect-app/prelude": "^0.29.1",
    "@effect-app/schema": "^0.27.0",
    "@effect/data": "^0.5.1",
    "@effect/io": "^0.12.0",
    "change-case": "^4.1.2",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "tsconfig-paths": "^4.1.2",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz"
  },
  "sideEffects": false
}
